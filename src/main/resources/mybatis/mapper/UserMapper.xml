<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.repo.mapper.UserMapper">


    <select id="getUserInfo" resultType="com.example.demo.entity.UserEntity">
        select * from tb_user where id = #{userid}
    </select>
    <select id="getUserList" resultType="com.example.demo.entity.UserEntity">
        select * from tb_user
    </select>

</mapper>